{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["standard-with-typescript", "prettier"],
  "overrides": [],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module",
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-console": "off",
    "semi": ["error", "always"],
    "quotes": "off",
    "no-param-reassign": "off",
    "import/no-cycle": "off",
    "strictNullChecks": true
  }
}
